{
  "version": 2,
  "name": "template-front",
  "builds": [
    {
      "use": "@vercel/static-build",
      "src": "package.json"
    }
  ],
  "routes": [
    {
      "src": "(.*).css",
      "dest": "/$1.css"
    },
    {
      "src": "(.*).js",
      "dest": "/$1.js"
    },
    {
      "src": "(.*).json",
      "dest": "/$1.json"
    },
    {
      "src": "(.*).png",
      "dest": "/$1.png"
    },
    {
      "src": "(.*).jpg",
      "dest": "/$1.jpg"
    },
    {
      "src": "(.*).webp",
      "dest": "/$1.webp"
    },
    {
      "src": "(.*).gif",
      "dest": "/$1.gif"
    },
    {
      "src": "(.*).mp4",
      "dest": "/$1.mp4"
    },
    {
      "src": "(.*).svg",
      "dest": "/$1.svg"
    },
    {
      "src": "(.*).woff",
      "dest": "/$1.woff"
    },
    {
      "src": "(.*).woff2",
      "dest": "/$1.woff2"
    },
    {
      "src": "(.*).ttf",
      "dest": "/$1.ttf"
    },
    {
      "src": "(.*).txt",
      "dest": "/$1.txt"
    },
    {
      "src": "/.*",
      "dest": "index.html"
    }
  ]
}