{
  "name": "templatefrontend",
  "version": "3.0.0",
  "description": "Template for React and SCSS with Webpack",
  "main": "index.js",
  "scripts": {
    "build:vendor": "webpack --config ./config/webpack.vendor.config.ts --mode production",
    "build:project": "webpack --config ./config/webpack.config.ts --mode production",
    "build": "npm run build:vendor && npm run build:project",
    "available:port": "python3 ./config/scripts/port.py",
    "start": "npm run available:port && webpack serve --config ./config/webpack.dev.config.ts --mode development",
    "deploy": "vercel --prod",
    "install:vercel": "npm i -g vercel",
    "analyzer": "npx webpack --profile --json > stats.json && npx webpack-bundle-analyzer stats.json ./dist",
    "postinstall": "ts-node ./config/scripts/checkPython.ts"
  },
